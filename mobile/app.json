{
  "expo": {
    "name": "Whatcha Bringin",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#fdf1d1"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.itwasmattgregg.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Whatcha Bringin needs access to your photos to let you add cover images to your potluck gatherings. For example, you can select a photo of your backyard BBQ setup to use as the cover image for your gathering, making it easier for guests to recognize your event."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fdf1d1"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ac762173-a729-4d18-b2d2-cc7412d8824b"
      }
    }
  }
}
